<Window x:Class="FileExplorer.WindowOptionsAV"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileExplorer"
        mc:Ignorable="d"
        Title="Ayudas Visuales" Height="308" Width="529" ResizeMode="NoResize">
    <Grid Margin="1,0,-1,0">
        <ComboBox x:Name="comboBox1" Canvas.Left="165" Canvas.Top="141" Width="168" Height="28">
            <ComboBox.Background>
                <LinearGradientBrush EndPoint="0,1">
                    <GradientStop Color="#FFF0F0F0"/>
                    <GradientStop Color="White" Offset="1"/>
                </LinearGradientBrush>
            </ComboBox.Background>
            <ComboBoxItem Tag="corte">CORTE</ComboBoxItem>
            <ComboBoxItem Tag="detallado">DETALLADO</ComboBoxItem>
            <ComboBoxItem Tag="doblez">DOBLEZ</ComboBoxItem>
            <ComboBoxItem Tag="general">GENERAL</ComboBoxItem>
            <ComboBoxItem Tag="maquinado">MAQUINADO</ComboBoxItem>
            <ComboBoxItem Tag="pintura">PINTURA</ComboBoxItem>
            <ComboBoxItem Tag="soldadura">SOLDADURA</ComboBoxItem>
        </ComboBox>
        <Button Content="Entrar" HorizontalAlignment="Center" Margin="0,186,0,0" VerticalAlignment="Top" Width="41"/>

    </Grid>
</Window>
